<h1>User Page</h1>
<h2>All Orders</h2>
<ul>
  <% orders.forEach((order) => { %>
    <li>
      Order ID: <%= order._id %><br>
      Customer: <%= order.customerName %><br>
      Total Amount: $<%= order.totalAmount.toFixed(2) %><br>
      <a href="/orders/<%= order._id %>">View Details</a>
    </li>
  <% }); %>
</ul>
